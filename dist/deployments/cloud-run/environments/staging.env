# Environment variables for the staging environment
export NODE_ENV="staging"
export LOG_LEVEL="info"
export PORT=8080

# Application specific environment variables
export API_URL="https://staging-api.example.com"
export DATABASE_URL="postgres://user:password@staging-db.example.com:5432/db"
export JWT_SECRET="staging-jwt-secret-placeholder"
export AUTH_ENABLED="true"

# Service configuration
export MIN_INSTANCES=1
export MAX_INSTANCES=5
export MEMORY="1Gi"
export CPU="1"
export TIMEOUT="300s"

# Additional environment-specific variables
export FEATURE_FLAGS_ENABLED="true"
export MONITORING_ENABLED="true"

